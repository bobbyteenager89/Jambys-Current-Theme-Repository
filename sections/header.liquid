  <script>
      document.body.style.paddingTop = document.getElementById('site-control').clientHeight + 'px';

      const dropdownThisMenu = document.getElementById('dropdown-this-menu');
      if (dropdownThisMenu) {
        dropdownThisMenu.addEventListener("click", function(evt){
          // Convert experiment condition becomes true
          window.navCollectionTest = true;
          window.shopAll = true;
          window._conv_q = window._conv_q || [];
          // window.shopAll experiment trigger
          window._conv_q.push(['executeExperiment', '100430985' ]);
          // window.navCollectionTest experiment trigger
          window._conv_q.push(["executeExperiment", "100443832" ]);

          if(!document.getElementsByClassName('menu_desktop nav-rows')[0].classList.contains("open")){
            document.getElementsByClassName('menu_desktop nav-rows')[0].classList.add("open")
            document.getElementById('blur-effect').classList.add("active")
          }
        })
      }

      const iconClose_ = document.getElementById('icon-close');
      if (iconClose_) {
        iconClose_.addEventListener("click", function(evt){
          document.getElementById('nav-rows').classList.remove("open")
          document.getElementById('blur-effect').classList.remove("active")
        })
      }

      let nav = document.getElementsByClassName("menu_item")
      for(let e of nav){
        e.addEventListener("click", function(evt){
          if(e.lastElementChild.classList.contains("open-block")){
            e.lastElementChild.classList.remove("open-block");
            e.nextElementSibling.classList.remove("active");
            e.classList.remove("active")
            return
          }

          for(let second of document.getElementsByClassName("menu_item")){
            second.lastElementChild.classList.remove("open-block");
            second.nextElementSibling.classList.remove("active");
            second.classList.remove("active")
          }
          e.classList.add("active")
          e.lastElementChild.classList.add("open-block");
          e.nextElementSibling.classList.add("active");

        })
      }

      const blurEffect_ = document.getElementById('blur-effect');
      if (blurEffect_) {
        blurEffect_.addEventListener("click", function(evt){
          document.getElementById('blur-effect').classList.remove("active")
          document.getElementById('nav-rows').classList.remove("open")
        })
      }

      document.getElementById('hamburguer-click').addEventListener("click", function(evt){
        window.shopAll = true;
        // Convert experiment condition becomes true
        window.navCollectionTest = true;
        window._conv_q = window._conv_q || [];
        window._conv_q.push(['executeExperiment', '100430985' ]);
        // window.navCollectionTest experiment trigger
        window._conv_q.push(["executeExperiment", "100443832" ]);
        let elementClick = document.getElementById('nav-mobile-div');
        console.log(elementClick);
        let elementStyle = window.getComputedStyle(elementClick);
        let elementDisplay = elementStyle.getPropertyValue('display');

        let second = document.getElementById('page-menu');
        let test = second.classList.contains("reveal");

        if( elementDisplay == "block" && !test ){
          console.log("experiment");
          window.clickableMenuExp = 1;
          window._conv_q = window._conv_q || [];
          window._conv_q.push(["executeExperiment", {{ "100411591" }} ]);
        }else{
        }
        let search = document.getElementById("search-bar-engine")
        if(search.classList.contains("open")){
          document.getElementById("nav-bar-search").classList.remove("open")
          search.classList.remove("open");
          document.body.classList.remove("scroll-disabled");
          if(document.getElementsByClassName("aa-Panel")[0]){
            document.getElementsByClassName("aa-Panel")[0].style.display = "none";
          }
        }



      })

      let submenuLinks = [...document.querySelectorAll('.dtc-submenu__font')];
      let shopAllLinksDisplay = document.querySelectorAll('[id*="dtc-shop-all"');

      if(getComputedStyle(shopAllLinksDisplay[0]).display == 'block'){
        submenuLinks.forEach((link) => {
          link.style.fontWeight = '500';
        });
      } else {
        submenuLinks.forEach((link) => {
          link.style.fontWeight = '700';
        });
      }

      let visitedSearch = false
      document.addEventListener("click", function(evt) {
        const nav_search = evt.target.closest("#search-bar-engine");
  
        if (nav_search) {
          const searchContainer = document.getElementById("nav-bar-search");
  
          if (nav_search.classList.contains("open")) {
            searchContainer.classList.remove("open");
            nav_search.classList.remove("open");
            document.body.classList.remove("scroll-disabled");
  
            const panel = document.getElementsByClassName("aa-Panel")[0];
            if (panel) {
              panel.style.display = "none";
            }
          } else {
            if (document.getElementById("page-menu")?.classList.contains("reveal")) {
              document.getElementById("hamburguer-click")?.click();
            }
  
            document.body.classList.add("scroll-disabled");
            nav_search.classList.add("open");
            searchContainer.classList.add("open");
            console.log(visitedSearch,"visited")
            if (!visitedSearch) {
              document.getElementById("autocomplete-0-input")?.focus();
              visitedSearch = true;
            }
  
            const panel = document.getElementsByClassName("aa-Panel")[0];
            if (panel) {
              panel.style.display = "block";
            }
          }
        }
      });
  </script>
