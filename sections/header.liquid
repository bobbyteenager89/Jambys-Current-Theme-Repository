  }

  .description-category{
    font-family: sofia-pro,Avenir,sans-serif;
    font-size: 12px;
    font-weight: 500;
    line-height: 16px;
    letter-spacing: 0em;
    text-align: center;
  }

  .menu_desktop{
    position: absolute !important;
    width: 450px !important;
    background: #F3F4F9 !important;
    top: 0px !important;
    max-height: 100vh !important;
    overflow: auto !important;
    left: -16px !important;
    -ms-overflow-style: none !important;
    scrollbar-width: none !important;
  }
  .contains-children .menu_desktop.nav-rows.open{
    display:block !important;
  }
  .contains-children .menu_desktop{
    display: none !important;
  }
  .menu_desktop{
    display:block
  }

  .menu_desktop .menu_item{
    padding: 24px 0px 24px 16px;
    cursor:pointer;
    border-top: 1px solid #EAECF0;
  }

  .menu_desktop .menu_item:hover{
    opacity:0.8
  }

  .menu_desktop .menu_item a{
    padding: 0 !important
  }
  .child_links_desktop{
    display:block  !important;
    padding: 16px 32px;
  }
  .child_links_desktop a{
    padding: 0px !important
  }

  .menu_desktop::-webkit-scrollbar {
    display: none !important;
  }

  .menu_desktop .menu-title-featured{
    padding-left:16px;
    font-size: 16px;
  }

  .menu_desktop.nav-rows{
    border-top: 0 !important;
    margin-top: -17px !important;
    padding-top: 16px !important;
    border-radius: 0px;
    z-index: 9999 !important;
  }

  .menu_desktop .nav-ctas__cta.new-collection{
    width: 343px !important;
    padding: 12px 18px 12px 18px !important;
    margin: 0px 0px !important;
    display: flex !important;
    position: fixed;
    bottom: 40px !important;
    padding-bottom: 12px !important;
    width: 420px !important;
    left: 0px;
    background: #F3F4F9 !important;
    border-top: 1px solid #EAECF0;
    padding-top: 24px !important;
    margin-right: 15px !important;
    margin-left: 15px !important;
    z-index: 9999;
  }

  .bottom_menu_desktop{
    position: fixed;
    display: flex;
    bottom: 0;
    padding-bottom: 24px;
    width: 450px;
    left: 0;
    gap: 8px;
    justify-content: center;
    background: #F3F4F9;
    background-color: #F3F4F9;
    z-index: 1;
  }

  .search-bar-engine.open img{
    content: url("https://cdn.shopify.com/s/files/1/0090/4128/1109/files/icon_car_hover.png?v=1689791028") !important;
  }

  .nav-bar-search.open{
    display:block !important;
  }
  .nav-bar-search{
    display: none;
    position: fixed;
    z-index: 99999;
    right: 0;
  }

  #nav-mobile-div{
    padding-top: 20px !important;
  }

  .this.contains-children .nav-rows li:first-child{
    display: block !important
  }




  .nav-footer-div{
    width: 100%;
    padding-right: 32px;
    padding-bottom: 16px;
    position: fixed;
    bottom: 0px;
    padding-bottom: 16px;
    background: white;
    z-index: 2;
  }

  .button-bottom-desktop{
    font-family: sofia-pro,Avenir,sans-serif;
    border-radius: 9999px;
    font-size: 1.25rem;
    line-height: 1.75rem;
    font-weight: 600;
    text-transform: uppercase;
  }

  .button-bottom-mobile{
    font-family: sofia-pro,Avenir,sans-serif;
    border-radius: 9999px;
    font-size: 18px;
    font-weight: 600;
    text-transform: uppercase;
  }

  .menu_desktop .menu_item-empty{
    padding: 24px 0px 24px 16px !important;
    cursor: auto;
    border-top: 1px solid #EAECF0;
  }

  .menu_desktop .menu_item-empty a {
    padding: 0 !important;
  }

  .menu_item-empty{
    padding: 12px 0px 12px 0px !important;
    border-top: 1px solid #EAECF0;
    cursor: auto;
  }

  .menu_desktop .nav-rows .open{
    opacity: 1;
      visibility: visible;
      max-height: 75vh;
      transition: opacity 0.4s, visibility 0.4s;
      flex-wrap: wrap;
      width: calc(100vw - 10rem);

  }
  .blur-open{
    height: 100vh;
    width: 100vw;
    position: fixed;
    /*background: white;
    opacity: 0.2;*/
    z-index: 25;
    top: 0px;
    left: 0px;
    background: hsla(0,0%,97.3%,.8);
    display:none
  }
  .blur-open.active{
    display: block !important;
  }

  .close-container{
    display: flex;
    justify-content: start;
    align-items: center;
    gap: 8px;
    width: 78px;
    cursor: pointer;
    margin-bottom: 32px;
    margin-left: 16px;
    margin-top: 3px;
  }

  #dropdown-this-menu{
    text-transform: uppercase;
    cursor: pointer;
    font-weight: 600;
    line-height: 21px;
  }
  #dropdown-this-menu::after{
    bottom: 2px!important;
  }

  .hover_menu{
    display:block !important
  }
  .clickable_menu{
    display:block !important
  }
  .not {
    display: none !important
  }

  #page-menu .nav-footer-links .nav-footer-links__link.new {
    border-top: 1px solid #EAECF0 !important;
  }


  #hover_menu_desktop{
    display: none
  }
  #hover-mobile-div{
    display: none
  }
  #clickable_menu_desktop{
    display: block
  }
  #nav-mobile-div{
    display: block
  }

  #dtc-shop-all-desktop-26, #dtc-shop-all-desktop-27, #dtc-shop-all-mobile-26, #dtc-shop-all-mobile-27 {
    display: none !important;
  }

  .dtc-child__links {
    width: 100%;
    display: flex !important;
    align-items: center;
    justify-content: space-between;
  }


  @media (max-width: 768px) {
  .scroll-disabled{
    overflow-y: hidden !important;
  }
  }
  /* DTC SHOPBYTPE Experiment */
  .child_links.dtc-child-link-alt{
    display: none;
  }
  .child_links.dtc-child-link-alt span{
    font-weight: normal !important;
  }
  .child_links.dtc-child-link-alt.active{
    display: block;
    font-weight:700;
  }

  .slider-announcement{
    overflow: hidden;
    position: relative;
    background: {{ announcement_block.settings.carousel_announcement_color }};
    color: {{ announcement_block.settings.font_announcement_color }};
    height: 40px;
    max-height: 40px;
    font-size: {{ announcement_block.settings.desktop_announcement_text_size }}px;
  }
  .slider-announcement .swiper-slide{
    display: flex;
    align-items: center;
    justify-content: center;
  }
  .slider-announcement .swiper-button-next::after, .slider-announcement .swiper-button-prev::after{
    font-size: {{ announcement_block.settings.desktop_announcement_text_size }}px;
  }
  .slider-announcement .swiper-button-next, .slider-announcement .swiper-button-prev{
    background: none;
    color: {{announcement_block.settings.carousel_arrows_color}};
  }
  @media (min-width: 769px) {
    .slider-announcement .swiper-button-next{
      left: 73%;
    }
    .slider-announcement .swiper-button-prev{
      left: 25%;
    }
    .mobile-carousel-item{
      display:none !important;
    }
  }
  @media (max-width: 768px){
    .slider-announcement{
      height: 40px;
      max-height: 40px;
      font-size: {{ announcement_block.settings.mobile_announcement_text_size }}px;
    }
    .slider-announcement .swiper-button-next::after, .slider-announcement .swiper-button-prev::after{
      font-size: {{ announcement_block.settings.mobile_announcement_text_size }}px !important;
    }
    .annoucement-text-container{
      padding-right: 35px;
      padding-left: 35px;
    }
    .desktop-carousel-item{
      display:none !important;
    }
  }
  {% if section.settings.font_color_intelligems == blank or section.settings.font_color_intelligems != 'rgba(0,0,0,0)' %}
    .intelligems-announcement-bar .ig-dp-message span{
      color: {{  section.settings.font_color_intelligems }} !important;
    }
  {% else %}  
    .intelligems-announcement-bar .ig-dp-message span{
      color: #36409a !important;
    }
  {% endif %}

  {% if section.settings.font_color_bottom_intelligems == blank or section.settings.font_color_bottom_intelligems != 'rgba(0,0,0,0)' %}
    .intelligems-announcement-bar .ig-dp-container div:last-child .ig-dp-message span{
      color: {{ section.settings.font_color_bottom_intelligems }} !important;
    }
  {% else %}  
    .intelligems-announcement-bar .ig-dp-container div:last-child .ig-dp-message span{
      color: #36409a !important;
    }
  {% endif %}

  @media (max-width: 768px) {
    .nav-bar-search{
    top: 105px;
    }
    #page-menu{
    margin-top: 40px;
    }
    
    .search-container{
    max-height: calc(100vh - 135px)
    }
    }
    
    #nav-mobile-div{
    padding-top: 20px;
    margin-top: 27px !important; 
    }
    
    .search-container{
    max-height: calc(100vh - 140px)
    }
    .intelligems-announcement-bar{
    display:none;
    }

    
</style>

{% comment %} A hidden form to handle when the checkout button is pressed on the add to cart popup {% endcomment %}
<form action="{{ routes.cart_url }}" method="post" id="cc-checkout-form"></form>

{% if template == 'gift_card' %}
  {% comment %} Gift card template logo {% endcomment %}
  <div itemscope itemtype="http://schema.org/Organization">
    <meta itemprop="name" content="{{ shop.name }}">
    <a data-cc-animate-click class="logo" href="{{ shop.url }}" itemprop="url">
      {% if section.settings.logo != blank %}
        {% assign double_logo_width = section.settings.logo_width | default: 250 | times: 2 %}
        {% capture img_size %}{% if double_logo_width > 2048 %}2048{% else %}{{ double_logo_width }}{% endif %}x{% endcapture %}
        <img src="{{ section.settings.logo | img_url: img_size }}" alt="{{ shop.name | escape }}" itemprop="logo">
      {% else %}
        {{ shop.name | escape }}
      {% endif %}
    </a>
  </div>

{% else %}
  {% assign featured_links = section.settings.nav_featured_link | split: ', ' %}

  {% capture logo_html %}
  <a data-cc-animate-click data-cc-animate class="logo {% if section.settings.alt_logo != blank %}logo--has-alt-logo{% endif %}" href="{{ routes.root_url }}"
     itemprop="url">

    {% if template == 'index' and section.settings.logo_h1 %}
      <h1 class="logo logo-h1">
    {% endif %}

    <meta itemprop="name" content="{{ shop.name }}">
    {% if section.settings.logo != blank %}
      {% assign double_logo_width = section.settings.logo_width | default: 250 | times: 2 %}
      {% capture img_size %}{% if double_logo_width > 2048 %}2048{% else %}{{ double_logo_width }}{% endif %}x{% endcapture %}
      <img src="{{ section.settings.logo | img_url: img_size }}" alt="{{ shop.name | escape }}" itemprop="logo"
           width="{{ section.settings.logo.width }}" height="{{ section.settings.logo.height }}"/>

      {% if section.settings.alt_logo != blank %}
      <span class="alt-logo">
        <img src="{{ section.settings.alt_logo | img_url: img_size }}" alt="{{ shop.name | escape }}" itemprop="logo"
             width="{{ section.settings.alt_logo.width }}" height="{{ section.settings.alt_logo.height }}"/>
      </span>
      {% endif %}
    {% else %}
      {{ shop.name | escape }}
    {% endif %}

    {% if template == 'index' and section.settings.logo_h1 %}
      </h1>
    {% endif %}
  </a>
  {% endcapture %}

  {%- capture announcement -%}
  {% if section.settings.show_announcement %}
  {% if template == 'index' or section.settings.announcement_home_only == false %}
  <div class="cc-announcement">
    {% if section.settings.announcement_link != blank %}<a data-cc-animate-click href="{{ section.settings.announcement_link }}">{% endif %}
      {% if section.settings.announcement_text != blank %}
        <div class="cc-announcement__inner hidden-mobile">
          {{ section.settings.announcement_text }}
        </div>
      {% endif %}
      {% if section.settings.announcement_text != blank %}
        <div class="cc-announcement__inner mobile-only">
          {{ section.settings.announcement_text_mobile }}
        </div>
      {% endif %}
    {% if section.settings.announcement_link != blank %}</a>{% endif %}
  </div>
  {% endif %}
  {% endif %}
  {%- endcapture -%}

  {% capture carousel_announcement %}

    {% if show_new_annoucement %}
      {% if template == 'index' or announcement_block.settings.carousel_announcement_home_only == false %}
        <div class="slider-announcement desktop-carousel-item">
          <div class="swiper-wrapper ">
            {% if announcement_block.settings.desktop_announcement_text_1 != blank %}
              <div class="swiper-slide">
                <div class="annoucement-text-container"> <p>{{announcement_block.settings.desktop_announcement_text_1 }}</p></div>
              </div>  
            {% endif %}
            {% if announcement_block.settings.desktop_announcement_text_2 != blank %}
              <div class="swiper-slide">
                <div class="annoucement-text-container">{{announcement_block.settings.desktop_announcement_text_2 }}</div>
              </div>  
            {% endif %}
            {% if announcement_block.settings.desktop_announcement_text_3 != blank %}
              <div class="swiper-slide">
                <div class="annoucement-text-container">{{announcement_block.settings.desktop_announcement_text_3 }}</div>
              </div>  
            {% endif %}
          </div>      
          <div class="swiper-button-prev"></div>
          <div class="swiper-button-next"></div>
      
          <div class="swiper-pagination"></div>
      
        </div>

        <div class="slider-announcement mobile-carousel-item">
          <div class="swiper-wrapper ">
            {% if announcement_block.settings.mobile_announcement_text_1 != blank %}
              <div class="swiper-slide">
                <div class="annoucement-text-container">{{ announcement_block.settings.mobile_announcement_text_1 }}</div>
              </div>  
            {% endif %}
            {% if announcement_block.settings.mobile_announcement_text_2 != blank %}
              <div class="swiper-slide">
                <div class="annoucement-text-container">{{ announcement_block.settings.mobile_announcement_text_2 }}</div>
              </div>  
            {% endif %}
            {% if announcement_block.settings.mobile_announcement_text_3 != blank %}
              <div class="swiper-slide">
                <div class="annoucement-text-container">{{ announcement_block.settings.mobile_announcement_text_3 }}</div>
              </div>  
            {% endif %}  
          </div>  
          <div class="swiper-button-prev"></div>
          <div class="swiper-button-next"></div>
      
          <div class="swiper-pagination"></div>
      
        </div>
        
      {% endif %}
    {% endif %}

  {% endcapture %}

  {%- capture inline_link_html -%}
    {% if section.settings.desktop_navigation == "inline" %}
      {% comment %}
        {% render "mega-menu-desktop" %}
      {% endcomment %}
      {% render "dtc-mega-menu-desktop" %}
    {% endif %}
  {%- endcapture -%}

  <div data-section-type="header" itemscope itemtype="http://schema.org/Organization">
    <div
      id="site-control"
      class="
        site-control inline icons
        {% if section.settings.desktop_navigation == "inline" %}nav-inline-desktop{% endif %}
        {% if section.settings.nav_positioning == "inline" %}scrolls-with-page{% endif %}
        {% if section.settings.nav_positioning == "sticky" or section.settings.nav_positioning == "peek" %}fixed{% endif %}
        {% if section.settings.nav_opacity == "opaque" %}nav-opaque{% endif %}
        {% if section.settings.show_announcement or show_new_annoucement %}has-announcement{% endif %}
        {% if section.settings.use_alt_logo_when_active %}alt-logo-when-active{% else %}main-logo-when-active{% endif %}
        {% if section.settings.desktop_navigation == "inline" %}{% if section.settings.show_locale_selector or section.settings.show_currency_selector %}has-localization{% endif %}{% endif %}
      "
      data-cc-animate
      data-opacity="{% if section.settings.nav_positioning == 'inline' and section.settings.nav_opacity contains 'opaque_on_scroll' %}opaque{% else %}{{ section.settings.nav_opacity }}{% endif %}"
      data-positioning="{{ section.settings.nav_positioning }}"
    >
      {{ announcement }}
      {{ carousel_announcement }}
      {% if section.settings.show_promo_bar %}
        <div class="intelligems-announcement-bar">
          <ig-volume-progress-bar-widget></ig-volume-progress-bar-widget>
        </div>
      {% endif %}
      <div class="links site-control__inner header-custom-color" id="nav-root">
        <a
          class="menu"
          href="#page-menu"
          aria-controls="page-menu"
          data-modal-nav-toggle
          aria-label="{{ 'layout.header.menu' | t | escape }}"
          id="hamburguer-click"
        >
          {% render 'icon-menu' %}
          <span class="text-link">{{ 'layout.header.menu' | t }}</span>
        </a>

        {{ inline_link_html }}

        {{ logo_html }}

        <div class="nav-right-side">
          {% if section.settings.desktop_navigation == 'inline' %}
            {% if section.settings.show_locale_selector or section.settings.show_currency_selector %}
              <div class="localization">
                {% render 'localization', input_prefix: 'header' %}
              </div>
