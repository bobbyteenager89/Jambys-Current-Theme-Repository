    <script>
      function loadAsyncScript(src, onLoad, timing) {
        var script = document.createElement('script');
        script.async = true;
        script.src = src;
        if (onLoad) {
          script.onload = onLoad;
        }
        function loadScript() {
          document.body.appendChild(script);
        }
        if (timing === 'afterLoad' && document.readyState !== 'complete') {
          window.addEventListener('load', loadScript);
        } else {
          loadScript();
        }
      }
    </script>
