        `${
          nav.dataset.opacity === 'opaque'
            ? window.innerHeight - nav.clientHeight
            : window.innerHeight
          }px`
      );
      document.documentElement.style.setProperty(
        '--viewport-height',
        nav.dataset.opacity.indexOf('opaque') > -1 && nav.dataset.positioning === 'sticky'
